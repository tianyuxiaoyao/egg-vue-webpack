{
  "name": "egg-vue-webpack",
  "version": "1.0.2",
  "description": "egg view plugin for webpack vue",
  "eggPlugin": {
    "name": "webpackVue"
  },
  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin"
  ],
  "dependencies": {
    "chokidar": "^2.0.4",
    "kcors": "^2.2.2",
    "koa": "^2.5.2",
    "memory-fs": "^0.4.1",
    "webpack": "^4.16.5",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.3"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg": "^2.10.0",
    "egg-bin": "^4.8.1",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.19.3",
    "eslint": "^5.4.0",
    "eslint-config-egg": "^7.0.0",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
